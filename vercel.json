{
    "version": 2,
    "builds": [
      {
        "src": "WeatherBug/wsgi.py",
        "use": "@vercel/python",
        "config": {
          "maxLambdaSize": "512mb", 
          "runtime": "python3.9"
        }
      },
      {
        "src": "collectstatic.sh",
        "use": "@vercel/shell", 
        "config": {
          "commands": [
            "python manage.py collectstatic --noinput" 
          ]
        }
      }
    ],
    "routes": [
      {
        "src": "/static/(.*)",
        "dest": "/static/$1"
      },
      {
        "src": "/(.*)",
        "dest": "WeatherBug/wsgi.py"
      }
    ]
  }
  